<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE variablelist SYSTEM "../../../dtd/dblite.dtd">
<variablelist>
	<varlistentry>
		<term><guilabel>HEAD</guilabel></term>
		<listitem>
			<para>
				Current commit checked out.
			</para>
		</listitem>
	</varlistentry>

	<varlistentry>
		<term><guilabel>Branch</guilabel></term>
		<listitem>
			<para>
				The latest commit of chosen branch.
			</para>
		</listitem>
	</varlistentry>

	<varlistentry>
		<term><guilabel>Tag</guilabel></term>
		<listitem>
			<para>
				The commit of chosen tag.
			</para>
		</listitem>
	</varlistentry>

	<varlistentry>
		<term><guilabel>Commit</guilabel></term>
		<listitem>
			<para>
				Any commit, you click <guibutton>...</guibutton> to launch log
				dialog to choose commit. You also can input commit hash, or
				friendly commit name, such as <literal>HEAD~4</literal>.
			</para>
		</listitem>
	</varlistentry>

</variablelist>
